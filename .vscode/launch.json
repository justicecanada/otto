    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "django"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Run Chat Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "django/tests/chat"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Python: Run Librarian Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "django/tests/librarian"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Django: Text Extractor Views Text",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "django/tests/text_extractor"
            ],
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Django: Eval Library QA",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/django/manage.py",
            "args": [
                "eval_responses",
                "mini_library_qa.yaml"
            ],
            "django": true
        },
        {
            "name": "Django: Reset App Data",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/django/manage.py",
            "args": [
                "reset_app_data",
                "apps"
            ],
            "django": true
        },
        {
            "name": "Django: Migrate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/django/manage.py",
            "args": [
                "migrate"
            ],
            "django": true
        },
        {
            "name": "Django: Sync Users",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/django/manage.py",
            "args": [
                "sync_users",
            ],
            "django": true
        },
        {
            "name": "Django: Run Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/django/manage.py",
            "args": [
                "runserver",
            ],
            "django": true,
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Django: Celery Worker",
            "type": "debugpy",
            "request": "launch",
            "module": "celery",
            "cwd": "${workspaceFolder}/django",
            "args": [
                "-A",
                "otto",
                "worker",
                "-l",
                "INFO",
                "--pool=gevent",
                "--concurrency=256"
            ],
            "env": {
                "GEVENT_SUPPORT": "True"
            }
        },
    ],
    "compounds": [
        {
            "name": "Django: Run Server & Celery Worker",
            "configurations": [
                "Django: Run Server",
                "Django: Celery Worker"
            ]
        }
    ]
}
